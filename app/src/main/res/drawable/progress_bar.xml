<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Define the background properties like color etc -->
    <item android:id="@android:id/background">
        <shape>
            <gradient
                android:startColor="@color/material_grey_600"
                android:centerColor="@color/material_grey_600"
                android:centerY="1.0"
                android:endColor="@color/material_grey_800"
                android:angle="270" />
        </shape>
    </item>

    <!-- Define the progress properties like start color, end color etc -->
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient
                    android:startColor="@color/quantum_googblue500"
                    android:centerColor="@color/quantum_googblue700"
                    android:centerY="1.0"
                    android:endColor="@color/quantum_googblue900"
                    android:angle="270" />
            </shape>
        </clip>
    </item>
</layer-list> 